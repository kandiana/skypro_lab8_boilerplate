{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base/legacy", "plugin:@typescript-eslint/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-plusplus": "off",
    "no-console": "error"
  }
}
